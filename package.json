{
  "name": "paper_trading",
  "version": "1.0.0",
  "description": "PaperTrading project",
  "main": "index.js",
  "repository": "https://github.com/biem97/PaperTrading.git",
  "author": "Son Nguyen <biem97@github.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "docker-compose up",
    "start:dev": "docker-compose -f docker-compose.dev.yml up",
    "clean": "docker-compose down -v",
    "clean:dev": "docker-compose -f docker-compose.dev.yml down -v --remove-orphans",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "lint": "eslint --fix web-app/src && eslint --fix web-server/src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "pinst": "^2.1.6"
  },
  "settings": {
    "jest": {
      "version": 26
    }
  }
}
